

<h1>create a blogger </h1>

<% if flash[:my_errors] %>
    <ul>
        <% flash[:my_errors].each do |error|%>
            <%= error %>
        <% end %>
    </ul>
<% end %>

<%=form_for @blogger do |f|%>

<%=f.label :name%>
<%=f.text_field :name%>
<br>
<%=f.label :bio%>
<%=f.text_field :bio%>
<br>
<%=f.label :age%>
<%=f.text_field :age%>
<br>
<%=f.submit%>


<%end%>


