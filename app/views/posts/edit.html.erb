<h1>Edit Post</h1>

 <%= form_for @post do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :content %>
    <%= f.text_field :content%>
    <br>
    <%= f.label :blogger_id %>
    <%= f.collection_select :blogger_id, Blogger.all, :id, :name%>
    <br>
    <%= f.label :destination_id %>
    <%= f.collection_select :destination_id, Destination.all, :id, :name %>
    <br>
    <%= f.submit %>
    
 <% end %>